#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int amtgiven,billamt;
    scanf("%d %d",&amtgiven,&billamt);
    printf("Quotient:%d\n",amtgiven/billamt);
    printf("Remainder:%d",amtgiven%billamt);
    

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
